#navbar-iframe{height:0;visibility:hidden;display:none;}
.blog-feeds, #blog-pager, .blog-pager, .post-footer, h3.post-title, #main{display:none !important;}

#vueApp {
  background: rgba(0,0,0,.05);
}
.header-section {
  height: auto;
  text-align: center;
  padding:10% 0;
}
#vueApp .card {
  margin:34px 0;
}
.credits {
  margin-top: 15px;
}
